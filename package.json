{
  "name": "noa-engine",
  "version": "0.23.5",
  "description": "Experimental voxel game engine",
  "main": "index.js",
  "scripts": {
    "start": "sleep 1 && open http://localhost:8080/ & webpack-dev-server -d --inline --content-base examples/hello-world/ examples/hello-world/index.js",
    "test": "sleep 1 && open http://localhost:8080/ & webpack-dev-server -d --inline --content-base examples/test/ examples/test/index.js",
    "doc": "cat tools/buildDocs.js | node"
  },
  "author": "Andy Hall",
  "keywords": [
    "voxel",
    "game",
    "engine"
  ],
  "license": "MIT",
