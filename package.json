    "aabb-3d": "andyhall/aabb-3d",
    "box-intersect": "^1.0.1",
    "ent-comp": "^0.4.1",
    "extend": "^3.0.0",
    "fast-voxel-raycast": "^0.1.1",
    "game-inputs": "^0.2.0",
    "game-shell": "^1.3.0",
    "gl-vec3": "^1.0.3",
    "ndarray": "^1.0.16",
    "ndarray-hash": "^1.0.0",
    "voxel-aabb-sweep": "^0.5.0",
    "voxel-physics-engine": "^0.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhall/noa"
  },
  "bugs": {
    "url": "https://github.com/andyhall/noa/issues"
